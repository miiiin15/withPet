<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Custom attributes for CustomButton -->
    <declare-styleable name="CustomButton">
        <attr name="android:text" />
    </declare-styleable>
    <declare-styleable name="CustomSelect">
        <attr name="bottomSheetHeight" format="dimension" />
        <attr name="type" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomEditText">
        <attr name="labelText" format="string" />
        <attr name="errorText" format="string" />
    </declare-styleable>
</resources>